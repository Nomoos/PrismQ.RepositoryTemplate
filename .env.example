# PrismQ Module Configuration Template
# Copy this file to .env and fill in your values

# Working Directory (automatically managed)
WORKING_DIRECTORY=

# Application Settings
APP_NAME=PrismQ.ModuleName
APP_ENV=development
DEBUG=true

# Logging Configuration
# Log levels: DEBUG, INFO, WARNING, ERROR, CRITICAL
LOG_LEVEL=INFO
# Optional: Specify a log file path for file logging (in addition to console)
# LOG_FILE=./logs/app.log

# Python Configuration
# Specify the Python executable to use for creating virtual environments
# Windows default: python
# Linux default: python3
# Examples: python, python3, python3.10, C:\Python310\python.exe, /usr/bin/python3.11
PYTHON_EXECUTABLE=python

# API Keys (if needed)
# OPENAI_API_KEY=your_openai_api_key_here
# ANTHROPIC_API_KEY=your_anthropic_api_key_here
# HUGGINGFACE_API_KEY=your_huggingface_api_key_here

# Database Configuration
# DATABASE_URL=sqlite:///./data/app.db

# Paths
# INPUT_DIR=./input
# OUTPUT_DIR=./output
# CACHE_DIR=./cache

# Hardware Configuration
# Target: Windows with NVIDIA RTX 5090, AMD Ryzen CPU, 64GB RAM
# CUDA_VISIBLE_DEVICES=0
# PYTORCH_CUDA_ALLOC_CONF=max_split_size_mb=512
